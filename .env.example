# ===== Global Routing Prefix =====
# Base path prepended to all API routes
GLOBAL_PREFIX=

# ===== Environment & CORS =====
# Application environment
NODE_ENV=
# Port where the application will listen
PORT=
# Comma-separated list of allowed CORS origins
CORS_ORIGINS=

# === Cookies ===
# Cookie domain, example: ".example.com"
COOKIE_DOMAIN=